import java.util.ArrayList;


public class Computer {

	static ArrayList<JBoardT> temp = new ArrayList<JBoardT>();
	static ArrayList<JBoardT> cBoard = new ArrayList<JBoardT>();
	static int score = 0;
	static JBoardT place;
	public static void startMove(ArrayList<JBoardT> board){
		
		System.out.println("at ai");
		temp = board;
		for(int i = 0; i<temp.size();i++)
		{
			JBoardT t = temp.get(i);
			cBoard.add(t);
			
			if(score<miniMax())
			{
				score=miniMax();
				place = t;
			}
			cBoard.remove(t);
		}
		
		Board.updateBoard(place);
		cBoard.add(place);
		
		if(cBoard.size()>=3)
		{
			MainFrame.victory(cBoard);
		}
	}
	
	private static int miniMax(){
		return 0;
	}
	
	private int[] coordinate(){
		return null;
	}
}
